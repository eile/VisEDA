namespace zeq.hbp;

table Frame
{
    /// starting frame number for an animation loop
  start:uint;
    /// current frame (wrapped into [start end])
  current:uint;
    /// end frame number for an animation loop
  end:uint;
    /// animation delta between start<->end
  delta:int;
}

root_type Frame;
